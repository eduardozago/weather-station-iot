    {
        "id": "4abfd86aef6c5504",
        "type": "postgresql",
        "z": "b144945c4a48b5fe",
        "name": "get_data",
        "query": "SELECT date_bin('1 minutes', sensors_info.ts, (to_timestamp($startDate) AT TIME ZONE 'America/Sao_Paulo')) as timebucket, avg(sensors_info.val) as avgval from sensors_info WHERE sensors_info.ts > (to_timestamp($startDate) AT TIME ZONE 'America/Sao_Paulo') AND sensors_info.ts < ((to_timestamp($startDate) AT TIME ZONE 'America/Sao_Paulo') + ($range * interval '1 minute')) AND sensors_info.st = $sensorType GROUP BY timebucket, st ORDER BY timebucket ASC;",
        "postgreSQLConfig": "bbbd892bf7f0b773",
        "split": false,
        "rowsPerMsg": 1,
        "outputs": 1,
