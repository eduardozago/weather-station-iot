    {
        "id": "d1bb6a3a26b54b3d",
        "type": "function",
        "z": "b144945c4a48b5fe",
        "name": "format_form_data",
        "func": "msg.queryParameters = {};\nmsg.queryParameters.startDate = msg.payload.startDate.split('T')[0];\nlet time = msg.payload.startTime;\ntime = (time.split('T')[1]).split('.')[0];\nmsg.queryParameters.startDate = msg.queryParameters.startDate+'T'+time+'Z';\nmsg.queryParameters.startDate = new Date(msg.queryParameters.startDate).getTime()/1000;\nmsg.queryParameters.range = msg.payload.range;\nmsg.queryParameters.sensorType = Number(msg.payload.sensorType);\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
